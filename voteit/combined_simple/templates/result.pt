<tal:main xmlns:i18n="http://xml.zope.org/namespaces/i18n" i18n:domain="voteit.combined_simple">
<div>
    <tal:iterate repeat="proposal proposals">
        <div tal:condition="complete" tal:define="result context.poll_result[proposal['uid']]" class="combined_simple_result">
            <div class="resultbox approve"><tal:trans i18n:translate="">Approve</tal:trans>: ${result['approve']}</div>
            <div class="resultbox deny"><tal:trans i18n:translate="">Deny</tal:trans>: ${result['deny']}</div>
            <div class="resultbox abstain"><tal:trans i18n:translate="">Abstain</tal:trans>: ${result['abstain']}</div>
            <div class="clear"><!-- --></div>
        </div>
        <tal:proposal
            replace="structure api.render_view_group(context, request, 'proposal', brain=proposal)">
            Proposal
        </tal:proposal>
    </tal:iterate>
 </div>
</tal:main>
